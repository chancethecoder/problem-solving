#include <iostream>
#include <cstdio>
#include <algorithm>

using namespace std;
const int MAX_N = 100000000;
int n;
int arr[MAX_N];

void proc()
{
    cin >> n;
    
    for(int i = 0; i < n; ++i)
    {
        scanf("%d", &arr[i]);
    }
    
    sort(arr, arr + n);

	if(n == 3) cout << arr[1] << arr[0] << "\n";
	else cout << arr[0] << arr[3] << "\n";
}

int main()
{
    proc();
    return 0;
}
